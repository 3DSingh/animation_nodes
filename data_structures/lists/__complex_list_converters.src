from mathutils import Vector
from . base_lists cimport FloatList

def getElementFromVector3DList(Vector3DList myList, long index):
    return Vector(myList.base.getSubTuple(index * 3, 3))

def elementToBaseListOfVector3DList(vector):
    cdef FloatList element = FloatList.fromValues(vector)
    if element.length != 3:
        raise TypeError("Invalid Vector Length. Expected 3.")
    return element
