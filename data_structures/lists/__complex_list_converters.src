from mathutils import Vector
from . base_lists cimport FloatList

def appendToVectorList(VectorList myList, vector):
    if len(vector) != 3: raise Exception("Invalid Vector Length. Expected 3.")
    myList.data.extend(vector)

def getElementFromVectorList(VectorList myList, long index):
    return Vector(myList.data.getSubTuple(index * 3, 3))
